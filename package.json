{
  "private": true,
  "workspaces": {
    "packages": [
      "api",
      "web",
      "packages/*"
    ]
  },
  "devDependencies": {
    "@redwoodjs/core": "0.41.0",
    "husky": "^7.0.0",
    "lint-staged": "^12.1.7"
  },
  "eslintConfig": {
    "extends": "@redwoodjs/eslint-config",
    "root": true
  },
  "engines": {
    "node": ">=14.17 <=16.x",
    "yarn": ">=1.15 <2"
  },
  "prisma": {
    "seed": "yarn rw exec seed"
  },
  "scripts": {
    "prepare": "husky install"
  },
  "dependencies": {},
  "lint-staged": {
    "**/*/*.{js,jsx,ts,tsx,json,css,scss,md}": [
      "npx prettier --write"
    ]
  }
}
